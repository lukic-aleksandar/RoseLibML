{
	"name": "rose-lib-ml",
	"displayName": "RoseLibML",
	"description": "RoseLibML extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rose-lib-ml.openPCFG",
		"onCommand:rose-lib-ml.openMCMC",
		"onCommand:rose-lib-ml.openIdioms"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rose-lib-ml.openPCFG",
				"title": "Open pCFG",
				"category": "RoseLibML"
			},
			{
				"command": "rose-lib-ml.openMCMC",
				"title": "Open MCMC",
				"category": "RoseLibML"
			},
			{
				"command": "rose-lib-ml.openIdioms",
				"title": "Open Idioms",
				"category": "RoseLibML"
			},
			{
				"command": "rose-lib-ml.runPCFG",
				"title": "Run pCFG",
				"category": "RoseLibML"
			},
			{
				"command": "rose-lib-ml.runMCMC",
				"title": "Run MCMC",
				"category": "RoseLibML"
			},
			{
				"command": "rose-lib-ml.generate",
				"title": "Generate",
				"category": "RoseLibML"
			}
		],
		"menus": {
			"commandPalette": [
			  {
				"command": "rose-lib-ml.runPCFG",
				"when": "false"
			  },
			  {
				"command": "rose-lib-ml.runMCMC",
				"when": "false"
			  },
			  {
				"command": "rose-lib-ml.generate",
				"when": "false"
			  }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}